    "test:types": "vue-tsc --build --force",
    "test:format": "prettier --check src/ e2e/",
    "test:linters": "npm run test:eslint && npm run test:types && npm run test:format",
    "format": "prettier --write src/ e2e/",
    "dev": "vite"
  },
  "dependencies": {
    "@arcgis/core": "^4.28.10",
