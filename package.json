    "@vue/tsconfig": "^0.5.0",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.4.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.32",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.11",
