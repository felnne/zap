    "ajv-formats": "^3.0.1",
    "axios": "^1.6.3",
    "markdown-it": "^14.0.0",
    "remove-markdown": "^0.5.0",
    "uuid": "^9.0.1",
    "v-clipboard": "^3.0.0-next.1",
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@playwright/test": "1.41.2",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tailwindcss/typography": "^0.5.10",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/markdown-it": "^13.0.7",
