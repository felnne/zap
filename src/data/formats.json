    "pdf_geo": {
      "slug": "pdf_geo",
      "name": "PDF",
      "description": "Download information as a PDF file with embedded georeferencing.",
      "extensions": [],
      "mediaTypes": ["application/pdf+geo"],
      "url": "https://metadata-resources.data.bas.ac.uk/media-types/application/pdf+geo"
